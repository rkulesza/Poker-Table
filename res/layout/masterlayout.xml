<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
android:id="@+layout/masterlayout"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
android:background="@drawable/layoutmaster">
>
<RelativeLayout
android:id="@+layout/botoeslayout"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:padding="8px"
>
<Button
android:id="@+button/buttonlogin"
android:layout_width="220px"
android:layout_height="65px"
android:background="@drawable/login"
android:layout_alignParentRight="true"
>
</Button>
<Button
android:id="@+button/buttonsettings"
android:layout_width="220px"
android:layout_height="65px"
android:background="@drawable/settings"
android:layout_alignParentRight="true"
android:layout_below="@+button/buttonlogin"
>
</Button>
<Button
android:id="@+button/buttonabout"
android:layout_width="220px"
android:layout_height="65px"
android:background="@drawable/about"
android:layout_alignParentRight="true"
android:layout_below="@+button/buttonsettings"
>
</Button>
<Button
android:id="@+button/buttonexit"
android:layout_width="220px"
android:layout_height="65px"
android:background="@drawable/exit"
android:layout_alignParentRight="true"
android:layout_below="@+button/buttonabout"
>
</Button>
</RelativeLayout>
</AbsoluteLayout>
